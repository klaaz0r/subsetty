{
  "name": "subsetty",
  "version": "0.0.1",
  "description": "subset fonts in node with fonttools",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "postinstall": "postinstall-build lib \"git submodule init && git submodule update\"",
    "build": "babel src/index.js -o dist/index.js",
    "test": "mocha src/index.test.js -w --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/klaaz0r/subsetty.git"
  },
  "keywords": [
    "subset",
    "fonts",
    "fonttools"
  ],
  "author": "klaaz0r",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "bluebird": "^3.4.7",
    "node-uuid": "^1.4.7",
    "postinstall-build": "^2.1.3",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "chai": "3.2.0",
    "mocha": "2.2.5"
  }
}
